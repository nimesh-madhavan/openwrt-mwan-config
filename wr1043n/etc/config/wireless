
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/qca956x_wmac'
	option country 'US'
	option legacy_rates '1'
	option channel '1'
	option htmode 'HT40'

config wifi-iface
	option encryption 'none'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option ssid 'Can_Masdeu'

